@page
@model SpotyWeb.Pages.CrearListaReproduccionModel
@{
}
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>CrearLista</title>
    <link rel="stylesheet" href="~/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/Registration-Form-with-Photo.css">
    <link rel="stylesheet" href="~/assets/css/styles.css">
</head>

<body style="background-color: rgb(19,49,56);">
    <div class="register-photo" style="background-color: rgb(19,49,56);">
        <div class="form-container">
            <div class="image-holder"></div>
            <form method="post" style="background-color: #133138;">
                <h2 class="text-center"><strong>Ingrese los datos de la lista de reproducción</strong></h2>
                <div class="form-group"><input type="text" class="form-control" name="Name" placeholder="Nombre De La Lista" /></div>
                <div class="form-group"><button class="btn btn-primary btn-block" type="submit" style="background-color: #8cd9f7;">Registrar Lista</button></div>
            </form>
        </div>
    </div>
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/bootstrap/js/bootstrap.min.js"></script>
</body>

</html>